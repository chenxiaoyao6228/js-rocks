{
  "name": "@js-rocks/react-tiny",
  "version": "0.1.0",
  "description": "react",
  "keywords": [
    "react"
  ],
  "main": "lib/react-tiny.cjs.js",
  "module": "lib/react-tiny.esm.js",
  "author": "ChenXiaoyao <chenxiaoyao6228@163.com> (http://www.chenxiaoyao.cn/)",
  "scripts": {
    "start": "webpack-dev-server --config ./build/webpack.dev.js",
    "build": "webpack --config ./build/webpack.prod.js",
    "prettier-write": "prettier --parser=typescript \"src/**/*.{js,ts}\" --write",
    "eslint-fix": "eslint \"src/**/*.{js,ts}\" --fix",
    "typecheck": "tsc",
    "build:watch": "rollup --watch -c rollup.config.js",
    "vite": "vitest",
    "test": "jest --selectProjects=react-tiny --c ../../jest.config.js",
    "test:watch": "jest --selectProjects=react-tiny --c ../../jest.config.js --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.10.1",
    "babel-loader": "^8.0.6",
    "core-js": "^3.6.4",
    "html-webpack-plugin": "^4.5.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@js-rocks/lodash-tiny": "workspace:*",
    "vitest": "^0.29.1"
  }
}
{
  "name": "@js-rocks/jsx-parser-tiny",
  "version": "0.0.1",
  "description": "a runtime jsx parser",
  "keywords": [
    "jsx parser"
  ],
  "license": "MIT",
  "author": "chenxiaoyao6228",
  "main": "lib/jsx-parser-tiny.cjs.js",
  "module": "lib/jsx-parser-tiny.esm.js",
  "types": "typings/index.ts",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "prettier-write": "prettier --parser=typescript \"src/**/*.{js,ts}\" --write",
    "eslint-fix": "eslint \"src/**/*.{js,ts}\" --fix",
    "typecheck": "tsc",
    "build": "rollup -c rollup.config.js",
    "build:watch": "rollup --watch -c rollup.config.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@js-rocks/lodash-tiny": "workspace:*"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7"
  }
}
